<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="animStyle" parent="@android:style/Widget.ProgressBar.Large">
        <item name="android:indeterminateDrawable">@anim/test</item>
    </style>
    <style name="rating_big_yellow" parent="@android:style/Widget.RatingBar">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:maxWidth">40.0dip</item>
        <item name="android:maxHeight">40.0dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@drawable/editcommentbar_c_yellow</item>
        <item name="android:progressDrawable">@drawable/editcommentbar_c_yellow</item>
        <item name="android:minWidth">40.0dip</item>
        <item name="android:minHeight">40.0dip</item>
        <item name="android:thumb">@null</item>
    </style>
    <style name="rating_small_yellow">
        <item name="android:maxHeight">15.0dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@drawable/ratingbar_small_yellow</item>
        <item name="android:progressDrawable">@drawable/ratingbar_small_yellow</item>
        <item name="android:minHeight">15.0dip</item>
        <item name="android:thumb">@null</item>
        <item name="android:isIndicator">true</item>
    </style>
    <style name="loading_bar">
        <item name="android:scaleType">fitCenter</item>
        <item name="android:indeterminateDrawable">@drawable/loading_bar_bg_green_normal</item>
        <item name="android:indeterminateDuration">2000</item>
        <item name="android:indeterminateBehavior">repeat</item>
    </style>
</resources>
